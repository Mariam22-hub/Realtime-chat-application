
{
    "extends": ["airbnb", "prettier", "plugin:node/recommended"],

    "rules": {
        "no-unused-vars": "off",
        "import/newline-after-import": 0,
        "func-names": "off",
        "class-methods-use-this": "off",
        "no-shadow": ["warn"],
        "prefer-const": "off",
        "consistent-return": "off"
    },

    "plugins": ["prettier"]
}